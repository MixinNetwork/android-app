<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MaterialSearchView">
        <attr name="searchCloseIcon" format="integer" />
        <attr name="searchBackIcon" format="integer" />
        <attr name="searchBarHeight" format="dimension" />
        <attr name="android:fitsSystemWindows" />
        <attr name="android:hint" />
        <attr name="android:textColor" />
        <attr name="android:textColorHint" />
        <attr name="android:inputType" />
    </declare-styleable>

    <declare-styleable name="TitleView">
        <attr name="titleText" format="string" />
        <attr name="rightIcon" format="reference" />
        <attr name="rightText" format="string" />
        <attr name="rightTextColor" format="color" />
        <attr name="leftIcon" format="reference" />
        <attr name="titleColor" format="color" />
        <attr name="android:background" />
        <attr name="need_divider" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="border_overlay" format="boolean" />
        <attr name="border_text_size" format="dimension" />
        <attr name="circle_background_color" format="color" />
    </declare-styleable>
    <declare-styleable name="PasswordView">
        <attr name="circleColor" format="color" />
    </declare-styleable>
    <declare-styleable name="CircleProgress">
        <attr name="size" format="dimension" />
        <attr name="progressWidth" format="dimension" />
        <attr name="playColor" format="color"/>
        <attr name="shadowColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="mProgress" format="integer" />
        <attr name="maxProgress" format="integer" />
        <attr name="stringSize" format="dimension"/>
        <attr name="border" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="VerificationCodeView">
        <attr name="vcv_count" format="integer" />
        <attr name="vcv_spacing" format="dimension" />
        <attr name="vcv_inputWidth" format="dimension" />
        <attr name="vcv_inputHeight" format="dimension" />
        <attr name="vcv_inputColor" format="color" />
        <attr name="vcv_textSize" format="dimension" />
        <attr name="vcv_textColor" format="color" />
    </declare-styleable>
    <declare-styleable name="PinView">
        <attr name="pin_color" format="color"/>
        <attr name="pin_count" format="integer"/>
        <attr name="pin_tipVisible" format="boolean"/>
        <attr name="pin_lineVisible" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="FlowLayout">
        <attr name="max_select" format="integer"/>
        <attr name="singleLine" format="boolean" />
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
        <attr name="flow_max_width" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="PlayView">
        <attr name="bg" format="color"/>
        <attr name="color" format="color"/>
    </declare-styleable>
    <declare-styleable name="PlayButton">
        <attr name="play_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="MessageLayout">
        <attr name="max_width" format="dimension" />
        <attr name="content_padding" format="dimension" />
    </declare-styleable>
    <declare-styleable name="AudioRecordView">
        <attr name="blink_color" format="color"/>
    </declare-styleable>

    <declare-styleable name="CallButton">
        <attr name="android:checkable" format="boolean"/>
        <attr name="android:checked" format="boolean"/>
        <attr name="bg_circle_checked" format="integer"/>
        <attr name="bg_circle_unchecked" format="integer"/>
        <attr name="ic_checked" format="integer"/>
        <attr name="ic_unchecked" format="integer"/>
        <attr name="ic_disable" format="integer"/>
    </declare-styleable>
    <declare-styleable name="KerningTextView">
        <attr name="kv_spacing" format="float" />
    </declare-styleable>
    <declare-styleable name="DraggableRecyclerView">
        <attr name="drag_direction" format="integer"/>
        <attr name="over_direction" format="integer"/>
    </declare-styleable>
    <declare-styleable name="RadioButton">
        <attr name="android:text" format="string"/>
        <attr name="android:checked" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="RoundTitleView">
        <attr name="title_text" format="string" />
        <attr name="right_icon" format="reference" />
        <attr name="left_icon" format="reference" />
    </declare-styleable>
    <declare-styleable name="TimeView">
        <attr name="text_color" format="reference" />
    </declare-styleable>
    <declare-styleable name="BalanceLayout">
        <attr name="center" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="AvatarsView">
        <attr name="avatar_border_width" format="dimension" />
        <attr name="avatar_border_color" format="color" />
        <attr name="avatar_size" format="dimension" />
        <attr name="avatar_rtl" format="boolean" />
    </declare-styleable>
    <declare-styleable name="AvatarsGroup">
        <attr name="avatar_group_size" format="dimension" />
        <attr name="avatar_group_margin" format="dimension" />
        <attr name="avatar_group_border_color" format="color" />
    </declare-styleable>
    <declare-styleable name="PlayerView">
        <attr name="use_controller" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="KeyboardLayout">
        <attr name="input_aera_id" format="reference"/>
    </declare-styleable>
    <declare-styleable name="SearchView">
        <attr name="circle_clear_icon" format="boolean" />
        <attr name="android:hint" format="string"/>
        <attr name="android:imeOptions" format="integer" />
    </declare-styleable>

    <declare-styleable name="WheelPicker">
        <attr name="wheel_data" format="reference" />
        <attr name="wheel_selected_item_position" format="integer" />
        <attr name="wheel_item_text_size" format="dimension" />
        <attr name="wheel_item_text_color" format="color" />
        <attr name="wheel_selected_item_text_color" format="color" />
        <attr name="wheel_same_width" format="boolean" />
        <attr name="wheel_maximum_width_text" format="string" />
        <attr name="wheel_maximum_width_text_position" format="integer" />
        <attr name="wheel_visible_item_count" format="integer" />
        <attr name="wheel_item_space" format="dimension" />
        <attr name="wheel_cyclic" format="boolean" />
        <attr name="wheel_indicator" format="boolean" />
        <attr name="wheel_indicator_color" format="color" />
        <attr name="wheel_indicator_size" format="dimension" />
        <attr name="wheel_curtain" format="boolean" />
        <attr name="wheel_curtain_color" format="color" />
        <attr name="wheel_atmospheric" format="boolean" />
        <attr name="wheel_curved" format="boolean" />
        <attr name="wheel_item_align" format="enum">
            <enum name="center" value="0" />
            <enum name="left" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="wheel_font_path" format="string" />
    </declare-styleable>
</resources>